.token.punctuation,
.token.operator,
.token.entity,
.token.url
	color: #333

.token.boolean,
.token.number,
.token.function
    color: #0092FF

.token.property,
.token.class-name,
.token.constant,
.token.symbol
    color: #37ae6f

.token.selector,
.token.important,
.token.atrule,
.token.keyword,
.token.builtin
    color: #ff0d68

.token.string,
.token.char,
.token.attr-value,
.token.regex,
.token.variable
    color: #833FE3
